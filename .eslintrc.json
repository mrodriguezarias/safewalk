{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-react",
    "prettier",
    "prettier/standard",
    "prettier/react"
  ],
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "jsx-quotes": ["error", "prefer-double"],
    "quotes": ["error", "double"],
    "semi": ["error", "never"],
    "space-before-function-paren": "off"
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
