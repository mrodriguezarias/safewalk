{
  "name": "safewalk",
  "version": "0.1.0",
  "private": true,
  "main": "app/src/index.js",
  "scripts": {
    "app:start": "cross-env NODE_ENV=local npm run script generate_expo_config && expo start",
    "app:android": "cross-env NODE_ENV=local npm run script generate_expo_config && expo start --android",
    "app:ios": "cross-env NODE_ENV=local npm run script generate_expo_config && expo start --ios",
    "api:start": "nodemon --watch api/src --exec babel-node ./api/src/index.js --env=local",
    "api:start:prod": "babel-node ./api/src/index.js --env=$NODE_ENV",
    "app:start:prod": "cross-env NODE_ENV=local npm run script generate_expo_config && expo start --no-dev --minify",
    "backend:start": "cross-env NODE_ENV=local webpack-dev-server --mode development --hot",
    "backend:build": "webpack -p",
    "sandbox": "nodemon --watch ./sandbox.js --exec babel-node ./sandbox.js",
    "script": "babel-node ./scripts"
  },
  "repository": {
    "url": "https://github.com/mrodriguezarias/safewalk"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@blackbox-vision/ra-language-spanish": "^3.5.0",
    "@react-native-community/async-storage": "~1.11.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/material-bottom-tabs": "^5.2.10",
    "@react-navigation/material-top-tabs": "^5.2.12",
    "@react-navigation/native": "^5.5.1",
    "@react-navigation/stack": "^5.5.1",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^4.11.1",
    "dotenv": "^8.2.0",
    "expo": "^38.0.0",
    "expo-constants": "~9.1.1",
    "expo-location": "~8.2.1",
    "express": "^4.17.1",
    "express-validation": "^3.0.5",
    "final-form": "^4.20.0",
    "helmet": "^4.1.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.13",
    "morgan": "^1.10.0",
    "ngraph.graph": "^19.1.0",
    "ngraph.path": "^1.3.1",
    "node-fetch": "^2.6.0",
    "prop-types": "^15.7.2",
    "qs": "^6.9.4",
    "ra-data-simple-rest": "^3.5.5",
    "react": "16.11.0",
    "react-admin": "^3.5.5",
    "react-dom": "16.11.0",
    "react-final-form": "^6.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.2.tar.gz",
    "react-native-appearance": "~0.3.3",
    "react-native-credit-card-input": "^0.4.1",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-maps": "0.27.1",
    "react-native-paper": "^4.1.0",
    "react-native-safe-area-context": "~3.0.7",
    "react-native-screens": "~2.9.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/node": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-expo": "^8.2.3",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.7",
    "nodemon": "^2.0.3",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.5",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
