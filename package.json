{
  "name": "safewalk",
  "version": "0.1.0",
  "private": true,
  "main": "app/src/index.js",
  "scripts": {
    "app:start": "NODE_ENV=local npm run _gec && expo start",
    "app:android": "NODE_ENV=local npm run _gec && expo start --android",
    "app:ios": "NODE_ENV=local npm run _gec && expo start --ios",
    "api:start": "nodemon --watch api/src --exec babel-node ./api/src/index.js --env=local",
    "api:start:prod": "babel-node ./api/src/index.js --env=$NODE_ENV",
    "backend:start": "NODE_ENV=local PORT=3500 webpack-dev-server --mode development --hot",
    "backend:build": "webpack -p",
    "sandbox": "nodemon --watch . --exec babel-node ./sandbox.js",
    "_gec": "babel-node ./generateExpoConfig.js"
  },
  "repository": {
    "url": "https://github.com/mrodriguezarias/safewalk"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "0.1.6",
    "@react-navigation/material-bottom-tabs": "^5.2.10",
    "@react-navigation/native": "^5.5.1",
    "@react-navigation/stack": "^5.5.1",
    "@use-expo/font": "^2.0.0",
    "bcrypt": "^4.0.1",
    "color": "^3.1.2",
    "command-line-args": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "expo": "^37.0.0",
    "expo-font": "~8.1.0",
    "expo-location": "~8.1.0",
    "expo-permissions": "~8.1.0",
    "expo-status-bar": "^1.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validation": "^3.0.5",
    "helmet": "^3.22.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.13",
    "morgan": "^1.10.0",
    "prop-types": "^15.7.2",
    "qs": "^6.9.4",
    "ra-data-simple-rest": "^3.5.5",
    "react": "16.9.0",
    "react-admin": "^3.5.5",
    "react-dom": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
    "react-native-appearance": "~0.3.3",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-maps": "0.26.1",
    "react-native-paper": "^3.10.1",
    "react-native-reanimated": "~1.7.0",
    "react-native-safe-area-context": "0.7.3",
    "react-native-screens": "~2.2.0",
    "react-native-web": "^0.11.7",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-form": "^8.3.6",
    "redux-thunk": "^2.3.0",
    "store": "^2.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/node": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-expo": "^8.1.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.7",
    "nodemon": "^2.0.3",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.5",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
